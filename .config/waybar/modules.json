{  
  "hyprland/workspaces": {
    "all-outputs": false,
    "warp-on-scroll": true,
    "format": "{name}",
    "active-only": true,
    "persistent_workspaces": {
      "1": {}, "2":{}, "3":{}, "4":{}, "5":{}
    }
  },

  "custom/notifications": {
    "tooltip-format": "Notifications Center",
    "format": "{icon}",
    "format-icons": {
      "none": "Ó™¢",
      "inhibited-none": "Ó™¢",
      "dnd-none": "Ó∞à",
      "dnd-inhibited-none": "Ó∞à",
      "notification": "ÓÆö",
      "inhibited-notification": "ÓÆö",
      "dnd-notification": "Ó∞â",
      "dnd-inhibited-notification": "Ó∞â"
    },
    "return-type": "json",
    "exec": "swaync-client -swb",
    "on-click": "swaync-client -t -sw",
    "escape": true
  },
  
  "hyprland/window": {
    "format": "{title}",
    "format-empty": " Destup",
    "max-length": 60,
    "separate-outputs": true,
    "icon-size": 0
  },
  
  "tray": {
    "spacing": 4,
    "icon-size": 16
  },
  
  "clock": {
    "format": "{:%I:%M %p}",
    "tooltip-format": "<big>{:%Y-%m-%d %H:%M:%S}</big>",
    "interval": 1
  },
  
  "pulseaudio": {
    "format": "{icon}  {volume}%",
    "format-muted": "Ôë¶  muted",
    "format-icons": {
      "headphone": "ÔÄ•",
      "hands-free": "ÔÄ•",
      "headset": "ÔÄ•",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®", "ÔÄ®"]
    },
    "on-click": "pactl set-sink-mute @DEFAULT_SINK@ toggle",
    "on-click-right": "pavucontrol",
    "scroll-up": "pactl set-sink-volume @DEFAULT_SINK@ +5%",
    "scroll-down": "pactl set-sink-volume @DEFAULT_SINK@ -5%"
  },
  
  "battery": {
    "states": {
        "warning": 30,
        "critical": 15
    },
    "format": "{icon}  {capacity}%",
    "format-full": "{icon}  {capacity}%",
    "format-charging": "Ôóß  {capacity}%",
    "format-plugged": "Ôá¶  {capacity}%",
    "format-alt": "{time}  {icon}",
    "format-icons": ["ÔâÑ", "ÔâÉ", "ÔâÇ", "ÔâÅ", "ÔâÄ"]
  },
  
  "network": {
    "interface-wifi": "wlan0", // Your Wi-Fi interface (e.g., wlan0, wlp3s0)
    "interface-ethernet": "eth0", // Your Ethernet interface (e.g., eth0, enp0s31f6)
    "interval": 5,
    "format-wifi": "{essid} ({signalStrength}%) {icon}",
    "format-ethernet": "Û∞àÄ {ifname}", // Ethernet: Cable icon + Interface name
    "format-disconnected": "Û∞§Ø Disconnected", // When no Wi-Fi or Ethernet connection
    "format-disabled": "Û∞ñ© Wi-Fi Off",    // When Wi-Fi radio is explicitly off
    "format-linked": "Û∞àÄ Wired (No IP)", // For wired, but no IP yet
    "format-vpn": "üîí VPN ({ifname})", // If NetworkManager's "network" module detects VPN
    "tooltip": true,
    "tooltip-format-wifi": "WiFi: {essid} ({signalStrength}%)\nIP: {ipaddr}/{cidr}\nUp: {bandwidthUpBytes} | Down: {bandwidthDownBytes}",
    "tooltip-format-ethernet": "Ethernet: {ifname}\nIP: {ipaddr}/{cidr}\nUp: {bandwidthUpBytes} | Down: {bandwidthDownBytes}",
    "tooltip-format-vpn": "VPN: {vpn_name} ({ipaddr})",
    "tooltip-format-disconnected": "No Network Connection",
    "tooltip-format-disabled": "Wi-Fi Disabled",
    "format-icons": {
        "wifi": [
            "Û∞§Ø", // 0-20%
            "Û∞§ü", // 20-40%
            "Û∞§¢", // 40-60%
            "Û∞§•", // 60-80%
            "Û∞§®"  // 80-100%
        ],
        "ethernet": "Û∞àÄ",
        "disconnected": "Û∞§Ø",
        "disabled": "Û∞ñ©",
        "vpn": "üîí" // VPN Lock icon (might not be used by "network" module's default format, but good to have)
    },
    "on-click": "~/.config/waybar/scripts/wifi.sh --wifi-menu",
  }
}
